PROGRAM fibonacci
  VÁLTOZÓK:
    a, b, n, f, i: EGÉSZ
 
  BE: n
  a := 0
  b := 1
  i := 1
  HA n = 0 AKKOR
    f := 0
  KÜLÖNBEN
    HA n = 1 AKKOR
      f := 1
    KÜLÖNBEN
      CIKLUS AMÍG i <= n - 1
        f := a + b
        a := b
        b := f
        i := i + 1
      CIKLUS_VÉGE
    HA_VÉGE
  HA_VÉGE
  KI: f
PROGRAM_VÉGE
